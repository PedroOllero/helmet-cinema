generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Movie {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  imageUrl    String
  genre       String
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String
}